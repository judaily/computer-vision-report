    "# csv 파일 읽기, 출력\n",
    "import csv # csv module\n",
    "\n",
    "f = open('./data.csv','r')\n",
    "rdr = csv.reader(f)\n",
    "\n",
    "for line in rdr: # rdr => [[],[],[]]\n",
    "    #print(line)\n",
    "    for s in line: # [] => s\n",
    "        print(s, end=' ')\n",
    "\n",
    "f.close()\n",
